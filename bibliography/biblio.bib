	journal = {Computer Physics Communications},
	keywords = {Relativistic Vlasov--Maxwell system, Asymptotic preserving scheme, Splitting scheme},
	pages = {13-26},
	title = {An asymptotic preserving scheme for the relativistic Vlasov--Maxwell equations in the classical limit},
