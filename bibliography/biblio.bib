	keywords = {Particle-in-cell, Geometric numerical methods, Discrete gradient, Vlasov--Maxwell},
	pages = {109890},
	title = {Energy-conserving time propagation for a structure-preserving particle-in-cell Vlasov--Maxwell solver},
